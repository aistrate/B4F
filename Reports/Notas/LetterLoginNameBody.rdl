    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="LetterLoginName">
      <Fields>
        <Field Name="PersonType">
          <DataField>PersonType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DearSirForm">
          <DataField>DearSirForm</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Email">
          <DataField>Email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LoginUserName">
          <DataField>LoginUserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyName">
          <DataField>CompanyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AccountNumbers">
          <DataField>AccountNumbers</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AccountNumbersFormatted">
          <Value>=Left(Fields!AccountNumbers.Value, Fields!LastCommaIndex.Value - 1) +
 Replace(Fields!AccountNumbers.Value, ", ", " en ", Fields!LastCommaIndex.Value)</Value>
        </Field>
        <Field Name="AccountsCount">
          <Value>=Split(Fields!AccountNumbers.Value, ",", -1, 0).GetLength(0)</Value>
        </Field>
        <Field Name="AccountsCountSuffix">
          <Value>=IIf(Fields!AccountsCount.Value = 1, "", "s")</Value>
        </Field>
        <Field Name="LastCommaIndex">
          <Value>=IIf(Fields!AccountsCount.Value &gt; 1,
        InStrRev(Fields!AccountNumbers.Value, ","),
        Len(Fields!AccountNumbers.Value) + 1)</Value>
        </Field>
        <Field Name="P1">
          <Value>="Wij hebben een nieuwe gebruikersnaam voor u aangemaakt: " +
Fields!LoginUserName.Value + ". " +
IIf(Fields!PersonType.Value = "Remisier Employee",
		Fields!P1_RemisierEmployee.Value,
		Fields!P1_Contact.Value)</Value>
        </Field>
        <Field Name="P1_Contact">
          <Value>="Hiermee kunt u uw " +
"portefeuille" + Fields!AccountsCountSuffix.Value + " " + 
Fields!AccountNumbersFormatted.Value +
" bij Paerel online volgen."</Value>
        </Field>
        <Field Name="P1_RemisierEmployee">
          <Value>="Hiermee kunt u de portefeuilles van uw cliënten bij Paerel online volgen."</Value>
        </Field>
        <Field Name="P2">
          <Value>=IIf(Fields!Email.Value &lt;&gt; "",
		Fields!P2_WithEmail.Value,
		Fields!P2_NoEmail.Value)</Value>
        </Field>
        <Field Name="P2_WithEmail">
          <Value>="Uit veiligheidsoverwegingen wordt uw wachtwoord apart verzonden " +
"naar uw e-mailadres: " +
Fields!Email.Value + "."</Value>
        </Field>
        <Field Name="P2_NoEmail">
          <Value>="Uit veiligheidsoverwegingen wordt uw wachtwoord apart verzonden naar uw e-mailadres. " +
"Helaas beschikken wij op dit moment niet over uw juiste e-mailadres. Wij verzoeken u " +
"vriendelijk uw e-mailadres te mailen naar servicedesk@paerel.nl."</Value>
        </Field>
        <Field Name="P3">
          <Value>="Om uw Effectengiro pagina te bezoeken gaat u naar http://www.mijnpaerel.nl. " +
"Op deze site klikt u op de link ""hier"". " +
"Op deze pagina wordt gevraagd om uw gebruikersnaam en wachtwoord in te voeren. " +
"Vervolgens komt u op uw persoonlijke beleggingspagina terecht. " +
IIf(Fields!PersonType.Value = "Remisier Employee",
		Fields!P3_RemisierEmployee.Value,
		Fields!P3_Contact.Value)</Value>
        </Field>
        <Field Name="P3_Contact">
          <Value>="Op deze pagina kunt u de samenstelling en waardeontwikkeling van " +
"uw portefeuille bekijken en volgen."</Value>
        </Field>
        <Field Name="P3_RemisierEmployee">
          <Value>="Op deze pagina kunt u de samenstelling en waardeontwikkeling van " +
"een portefeuille van uw cliënt bekijken en volgen."</Value>
        </Field>
        <Field Name="P4">
          <Value>="Mocht u nog vragen hebben dan kunt u contact met ons opnemen via " +
"servicedesk@paerel.nl" +
IIf(Fields!PersonType.Value = "Remisier Employee",
		" of op telefoonnummer 020-512 8750",
		"") +
"."</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AdoNetDataSet</DataSourceName>
        <CommandText>LetterLoginName</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DataSource">
            <Value>=Parameters!DataSource.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
